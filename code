aa=read.csv("env2.csv",header=T,row.names = 1)
df=aa
#设置6个潜变量进行路径分析
quality<- c(0,0,0,0,0,0,0)
funtiongenus<- c(1,0,0,0,0,0,0)
labialC<- c(1,1,0,0,0,0,0)
respiration<- c(1,1,1,0,0,0,0)
labial15N<- c(1,1,1,1,0,0,0)
Nloss<-c(1,1,1,1,0,0,0)
plant<-c(1,1,1,1,0,0,0)
fix_path <- rbind(quality,funtiongenus,labialC,respiration,labial15N,Nloss,plant)
#根据df选择对应的观察变量
fix_blocks = list(c(18,19,23),c(8,9,11),c(1,2,4),6,c(15,24),16,17)
#设置指定的关联，数值根据向量的行数修改，这个为6行
fix_modes = rep("A",7)
#进行plspm分析
fix_pls = plspm(df, fix_path, fix_blocks, modes = fix_modes,boot.val = TRUE)
#将方程可视化
innerplot(fix_pls, colpos = 'red', colneg = 'blue', show.values = TRUE, lcol = 'gray20', box.lwd = 0)
